{
    "name": "vinevax/laravel-dashboard-uptime-robot-tile",
    "description": "A tile for Laravel Dashboard that displays Uptime Robot monitors.",
    "keywords": [
        "vinevax",
        "laravel-dashboard-uptime-robot-tile",
        "uptime robot"
    ],
    "homepage": "https://github.com/vinevax/laravel-dashboard-uptime-robot-tile",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "spatie/laravel-dashboard": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "VineVax\\UptimeRobotTile\\": "src"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "VineVax\\UptimeRobotTile\\UptimeRobotTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
